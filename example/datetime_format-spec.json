          "time_only": "=substring(@(1,datetime),11,19)",
          "datetime": "=concat(@(1,date_only),'T',@(1,time_only),'Z')"
        }
      }
    }
  },
  {
    "operation": "remove",
    "spec": {
      "data": {
        "*": {
          "date_only": "",
          "only": ""
        }
      }
    }
  }
]
